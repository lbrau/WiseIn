<section class="panel">
    <header class="panel-heading">
        Liste des biens immobiliers
    </header>
    <table class="table table-striped table-advance table-hover">
        <thead>
            <tr>
                <th><i class="fa fa-bullhorn"></i> Photo du bien</th>
                <th class="hidden-phone"><i class="fa fa-question-circle"></i> Locataire</th>
                {#<th class="hidden-phone">Status</th>#}
                <th><i class="fa fa-bookmark"></i> Loyer</th>
                <th><i class="fa fa-bookmark"></i> Bail</th>
                <th><i class=" fa fa-edit"></i> Etat</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for bien in biens %}
            {% include 'wiseOwnerBundle:templates:appart_small_grid.html.twig' with {'cols': cols, 'bien': bien} only %}
            {#{% include 'wiseOwnerBundle:templates:appart_grid_line.html.twig' with {'cols': cols, 'bien': bien} only %}#}
        {% endfor %}
        </tbody>
    </table>
</section>